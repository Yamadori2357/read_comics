<%= provide(:title, '検索（漫画）') %>

<div class="search">
<%= form_with  method: :get, local: true do |f| %>
  <%= f.text_field :name, placeholder: "ユーザー検索"%>
<% end %>
</div>
<%= will_paginate %>

<ul class="users">
  <% @users.each do |user| %>
    <%= render user %>
  <% end %>
</ul>

<%= will_paginate %>

